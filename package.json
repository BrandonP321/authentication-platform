{
  "name": "fitness-tracker",
  "packageManager": "yarn@3.2.4",
  "engines": {},
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "web": "yarn workspace @fitness-tracker/web",
    "shared": "yarn workspace @fitness-tracker/shared",
    "web:start": "yarn concurrently --kill-others-on-fail -n \"WEB\" -c \"cyan\" \"yarn web dev\""
  },
  "devDependencies": {
    "concurrently": "^7.4.0"
  }
}
